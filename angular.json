      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "strict": true
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }

{
  "name": "memebox",
  "version": "0.0.1",
  "description": "A meme-management tool",
  "author": {
    "name": ""
  },
  "keywords": [],
  "main": "out-electron/main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "cpy": "cpy",
    "start": "npm-run-all -p ng:serve",
    "start:electron": "npm-run-all -p electron:serve ng:serve",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "build": "ng build --base-href ./",
    "ng:serve": "ng serve",
    "ng:serve:web": "ng serve -c web",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "start:app": "npm run ng:serve:web",
    "start:server": "ts-node --compiler ttypescript --project server/tsconfig.server.json server/server.ts",
    "start:all": "npm-run-all -p start:app start:server",
    "build:server": "ttsc -p server/tsconfig.server.json && cd out-tsc/server && npx @vercel/ncc build server/server.js -o ../../ncc -m --stats-out ../../ncc/stats.json",
    "build:prepare:folder": "cpy ncc/index.js release --rename=memebox.js ",
    "build:prepare": "npm run build:prod && npm run build:server && npm run build:prepare:folder",
    "build:windows": "npx nexe release/memebox.js -t windows-x64 -r dist -o release/out/memebox.exe",
    "build:linux": "npx  nexe release/memebox.js -t linux-x64 -r dist -o release/out/memebox-linux",
    "build:macos": "npx  nexe release/memebox.js -t mac-x64 -r dist -o release/out/memebox-macos",
    "build:all": "npm run build:windows && npm run build:linux && npm run build:macos",
    "build:with-electron": "npm run electron:serve-tsc && npm run build",
    "electron:serve-tsc:watch": "ttsc -p tsconfig-electron.json -w",
    "electron:serve-tsc": "npm-run-all -p electron:serve-tsc:watch electron:local",
    "electron:serve": "npx rimraf out-electron &&  wait-on tcp:4200 && npm run electron:serve-tsc",
    "electron:local": "wait-on out-electron/main.js && npx electron . --serve",
    "electron:build": "npm run build:prod && ttsc -p tsconfig-electron.json &&  electron-builder build",
    "electron:build:only": "ttsc -p tsconfig-electron.json &&  electron-builder build -p never"
  },
  "devDependencies": {
